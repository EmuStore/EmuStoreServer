generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:data.db"
}

model Config {
  id            Int    @id @default(autoincrement())
  propertyName  String @unique
  propertyValue String
}

model Game {
  id       Int    @id @default(autoincrement())
  name     String
  path     String
  size     BigInt
  platform String
}
