// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:data.db"
}

model Config {
  id            Int    @id @default(autoincrement())
  propertyName  String @unique
  propertyValue String
}
